{
  "name": "thirty_site",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:netlify": "netlify dev",
    "build": "tsc -b && vite build",
    "test": "jest --passWithNoTests",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "preview": "vite preview",
    "dep:graph": "madge --image full-graph.dot --extensions ts,tsx,js,jsx src && madge --json --extensions ts,tsx,js,jsx > full-dependency-map.json src",
    "flow:update": "node scripts/flow-from-json.mjs",
    "analyze": "npm run build && npx vite-bundle-analyzer"
  },
  "dependencies": {
    "@daily-co/daily-js": "^0.81.0",
    "@daily-co/daily-react": "^0.23.1",
    "@supabase/supabase-js": "^2.52.1",
    "dompurify": "^3.2.6",
    "eslint-plugin-react": "^7.37.5",
    "flag-icons": "^7.5.0",
    "framer-motion": "^12.23.11",
    "immer": "^10.1.1",
    "jotai": "^2.12.5",
    "moment": "^2.30.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.7.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@netlify/functions": "^4.1.15",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.19",
    "babel-jest": "^29.7.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "madge": "^8.0.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "rollup-plugin-analyzer": "^4.0.0",
    "supabase": "^2.33.7",
    "tailwindcss": "^3.4.3",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.1",
    "typescript": "v5.8.3",
